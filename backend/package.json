{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run compile",
    "compile": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/api/server.js",
    "serve": "node -r dotenv-flow/config build/app/index.js --node-env=development",
    "start:dev": "node -r dotenv-flow/config build/api/server.js --node-env=development",
    "watch:dev": "nodemon",
    "debug:dev": "nodemon -r dotenv-flow/config --inspect=9229 build/api/server.js --node-env=development",
    "lint": "tslint --config tslint.json --project tsconfig.json && npm run typecheck",
    "lint:fix": "tslint --config tslint.json --project tsconfig.json --fix && npm run typecheck",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "TS_NODE_PROJECT='tsconfig.json' mocha --require ts-node/register --require dotenv-flow/config --recursive '__tests__/**/*.spec.ts' --node-env=test",
    "drop:test": "node -r dotenv-flow/config build/db/tasks/drop.js --node-env=development",
    "seed:test": "node -r dotenv-flow/config build/db/tasks/seed.js --node-env=development",
    "import:test": "node -r dotenv-flow/config build/import/start.js --node-env=development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rxjs": "^6.4.0",
    "serve-favicon": "^2.5.0",
    "util-promisify": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/mockery": "^1.4.29",
    "@types/mongoose": "^5.3.26",
    "@types/node": "^11.13.4",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.41.6",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "css-loader": "^3.4.2",
    "dotenv-flow": "^3.1.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "mais-mongoose-seeder": "^1.0.7",
    "mocha": "^7.0.1",
    "mocha-typescript": "^1.1.17",
    "mockery": "^2.1.0",
    "nodemon": "^2.0.2",
    "proxyquire": "^2.1.0",
    "rxjs-tslint": "^0.1.7",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "style-loader": "^1.1.3",
    "supertest": "^4.0.2",
    "svg-url-loader": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}