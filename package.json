{
  "name": "predictor",
  "version": "1.0.0",
  "description": "Predictor",
  "main": "index.js",
  "scripts": {
    "test": "TS_NODE_PROJECT='backend/tsconfig.json' mocha --require ts-node/register backend/__tests__/**/*.spec.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"test/**/*.ts\"",
    "lb": "npm run lint:tslint:backend",
    "lfb": "npm run lint:tslint:fix:backend",
    "lint:tslint:backend": "tslint --config backend/tslint.json --project backend/tsconfig.json && npm run typecheck:backend",
    "lint:tslint:fix:backend": "tslint --config backend/tslint.json --project backend/tsconfig.json --fix && npm run typecheck:backend",
    "tb": "npm run typecheck:backend",
    "typecheck:backend": "tsc --project backend/tsconfig.json --noEmit",
    "cb": "npm run compile:backend",
    "compile:backend": "tsc --project backend/tsconfig.json",
    "dsit": "npm run drop:seed:import:test",
    "dst": "npm run drop:seed:test",
    "drop:seed:import:test": "NODE_ENV=test&& npm run compile:backend && node dist/db/tasks/drop.js && node dist/db/tasks/seed.js && node dist/import/start.js",
    "import:test": "NODE_ENV=test&& npm run compile:backend && node dist/import/start.js",
    "drop:seed:test": "NODE_ENV=test&& npm run compile:backend && node dist/db/tasks/drop.js && node dist/db/tasks/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckagiri/predictor.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckagiri/predictor/issues"
  },
  "homepage": "https://github.com/ckagiri/predictor#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^2.4.2",
    "config": "^3.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rxjs": "^6.4.0",
    "serve-favicon": "^2.5.0",
    "util-promisify": "^2.1.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/mockery": "^1.4.29",
    "@types/mongoose": "^5.3.26",
    "@types/node": "^11.13.4",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mais-mongoose-seeder": "^1.0.7",
    "mocha": "^7.0.1",
    "mocha-typescript": "^1.1.17",
    "mockery": "^2.1.0",
    "prettier": "^1.16.4",
    "proxyquire": "^2.1.0",
    "rxjs-tslint": "^0.1.7",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.5.3"
  }
}
