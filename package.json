{
  "name": "predictor",
  "version": "1.0.0",
  "description": "Predictor",
  "main": "index.js",
  "scripts": {
    "postinstall": "rm -rf node_modules/@types/react-native",
    "prebuild": "npm run build:clean",
    "build": "NODE_ENV=production webpack --mode=production --config ./config/webpack.client.prod.js --color -p --progress --hide-modules --display-optimization-bailout && npm run compile",
    "build:dev": "NODE_ENV=development webpack --mode=development --config ./config/webpack.client.dev.js && npm run compile",
    "build:clean": "rimraf ./dist && rimraf ./backend/build",
    "start": "node -r dotenv/config backend/build/api/server.js",
    "start:production": "npm run build && npm run start",
    "start:dev": "node -r dotenv-flow/config backend/build/api/server.js --node-env=development",
    "start:development": "npm run build:dev && npm run start:dev",
    "development": "nodemon",
    "compile": "tsc --project backend/tsconfig.json",
    "compile:start": "concurrently \"npm run compile\" \"npm run start:dev\"",
    "cs": "npm run compile:start",
    "debug": "nodemon -r dotenv-flow/config --inspect=9229 backend/build/api/server.js --node-env=development",
    "test": "TS_NODE_PROJECT='backend/tsconfig.json' mocha --require ts-node/register backend/__tests__/**/*.spec.ts",
    "prettify": "prettier --write \"**/*.ts\"",
    "lb": "npm run lint:tslint:backend",
    "lfb": "npm run lint:tslint:fix:backend",
    "lint:tslint:backend": "tslint --config backend/tslint.json --project backend/tsconfig.json && npm run typecheck:backend",
    "lint:tslint:fix:backend": "tslint --config backend/tslint.json --project backend/tsconfig.json --fix && npm run typecheck:backend",
    "tb": "npm run typecheck:backend",
    "typecheck:backend": "tsc --project backend/tsconfig.json --noEmit",
    "dsit": "npm run drop:seed:import:test",
    "dst": "npm run drop:seed:test",
    "drop:seed:import:test": "NODE_ENV=test node backend/build/db/tasks/drop.js && node backend/build/db/tasks/seed.js && node backend/build/import/start.js",
    "import:test": "NODE_ENV=test node backend/build/import/start.js",
    "drop:seed:test": "NODE_ENV=test node backend/build/db/tasks/drop.js && node backend/build/db/tasks/seed.js"
  },
  "lint-staged": {
    "*.ts": "lint:tslint:fix:backend"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:tslint:fix:backend"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckagiri/predictor.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckagiri/predictor/issues"
  },
