{
  "name": "predictor",
  "version": "1.0.0",
  "description": "Predictor",
  "main": "index.js",
  "scripts": {
    "install": "npm run install:backend",
    "install:backend": "cd backend && npm install",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf dist && rimraf backend/build",
    "build": "npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm run start",
    "build:start": "npm run build && npm run start",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "typecheck": "cd backend && npm run typecheck",
    "serve": "serve -s dist",
    "test": "cd backend && npm run test",
    "lint:be": "cd backend && npm run lint",
    "lintfix:be": "cd backend && npm run lint:fix",
    "dropseed:test": "cd backend && npm run drop:test && npm run seed:test",
    "dropseedimport:test": "cd backend && npm run drop:test && npm run seed:test && npm run import:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckagiri/predictor.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckagiri/predictor/issues"
  },
  "homepage": "https://github.com/ckagiri/predictor#readme",
  "dependencies": {},
  "devDependencies": {
    "dotenv": "^7.0.0",
    "dotenv-flow": "^3.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "12.16"
  }
}
